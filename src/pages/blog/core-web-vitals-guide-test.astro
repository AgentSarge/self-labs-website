---
import BaseLayout from "@/layouts/BaseLayout.astro";

const title = "Core Web Vitals: Complete Guide to Website Performance | Self Labs"
const description = "Master Core Web Vitals optimization with our comprehensive guide. Learn LCP, FID, CLS metrics and practical techniques to improve your website's performance and SEO rankings."
const canonical = "https://self-labs.io/blog/core-web-vitals-guide"
const publishDate = "2024-12-19"
const author = "Will Sargent"
---

<BaseLayout 
  title={title}
  description={description}
  canonical={canonical}
  ogImage="/blog/core-web-vitals-guide.jpg"
  ogType="article"
>
  <main class="px-4 py-16 md:px-8 lg:px-12">
    <div class="mx-auto max-w-4xl">
      <h1 class="text-4xl md:text-5xl font-bold text-white mb-6">
        Core Web Vitals: Complete Guide to Website Performance
      </h1>
      <p class="text-xl text-gray-300 leading-relaxed">
        Test content
      </p>
    </div>
  </main>
</BaseLayout>
